{
  "name": "typescript-node",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && set NODE_ENV=production&& node dist/src/index.js",
    "start:dev": "set NODE_ENV=development&& ts-node-dev src/index.ts",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts test/**/*.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Helioty/node-typescript.git"
  },
  "keywords": [
    "node.js",
    "typescript",
    "api"
  ],
  "author": "Helio Souza",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Helioty/node-typescript/issues"
  },
  "homepage": "https://github.com/Helioty/node-typescript#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "0.0.38",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.14.16",
    "@types/oracledb": "^5.1.0",
    "@types/supertest": "^2.0.10",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "config": "^3.3.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.10.8",
    "module-alias": "^2.2.2",
    "oracledb": "^5.1.0",
    "swagger-jsdoc": "^6.0.1",
    "swagger-ui-express": "^4.1.6"
  }
}
